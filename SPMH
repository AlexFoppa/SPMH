@prefix : <http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/> .

<http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH> rdf:type owl:Ontology ;
    owl:versionInfo "1.0" ;
    rdfs:comment "Ontology for personal mental health."@en .


#################################################################
#    Object Properties
