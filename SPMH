###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH#hasContext
:hasContext rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :isAbout ;
            rdfs:domain :Person ;
            rdfs:range :Context ;
            rdfs:comment "A person may have several contexts" ;
            rdfs:label "hasContext" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/creates
:creates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :isResultOf ;
         rdfs:domain :Procedure ;
         rdfs:range :Artifact ;
         rdfs:comment "A procedure can create an artifact" ;
         rdfs:label "creates" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/finds
:finds rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       owl:inverseOf :isFoundUsing ;
       rdfs:domain :Procedure ;
       rdfs:range :Trace ;
       rdfs:comment "A procedure can find a trace" ;
       rdfs:label "finds" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/hasA
:hasA rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      owl:inverseOf :isFoundIn ;
      rdfs:domain :Context ;
      rdfs:range :Trace ;
      rdfs:comment "A context may have 0 to multiple traces of depression" ;
      rdfs:label "containsTrace" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/hasOriginIn
:hasOriginIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isPreservedAs ;
             rdfs:domain :Artifact ;
             rdfs:range :Publication ;
             rdfs:comment "An artifact is generated based on a social media publication" ;
             rdfs:label "hasOriginIn" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isAbout
:isAbout rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Context ;
         rdfs:range :Person ;
         rdfs:comment "A context is about a specific person" ;
         rdfs:label "isAbout" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isFoundIn
:isFoundIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Trace ;
           rdfs:range :Context ;
           rdfs:comment "A trace is found in a specific publication or medical record" ;
           rdfs:label "isFoundIn" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isFoundUsing
:isFoundUsing rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Trace ;
              rdfs:range :Procedure ;
              rdfs:comment "A trace is found using a specific procedure" ;
              rdfs:label "isFoundUsing" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isOwned
:isOwned rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :owns ;
         rdfs:domain :Account ;
         rdfs:range :Person ;
         rdfs:comment "A social media account has a person as an owner" ;
         rdfs:label "isOwnedBy" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isPreservedAs
:isPreservedAs rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Publication ;
               rdfs:range :Artifact ;
               rdfs:comment "An artifact generated to preserve a social media publication" ;
               rdfs:label "isPreservedAs" .


###  http://www.semanticweb.org/alfoppa/ontologies/2024/3/SPMH/isResultOf
